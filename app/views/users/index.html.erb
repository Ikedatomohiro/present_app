
<% content_for :title do %>
  Users list
<% end %>
<div class="container">

  <% @users.each do |user| %>
  <%= link_to(user.id, "/users/#{user.id}") %>
  <%= user.name %>
  <%= user.email %>
  <%= user.password %>
  <%= user.gender %>
  <%= user.birthday %>
  <%= user.email %>
  <br>
  <% end %>

    
</div>